{
  "name": "whiteboard-react",
  "version": "2.0.0",
  "description": "Whiteboard for react",
  "author": "Sandeep",
  "license": "ISC",
  "repository": "https://github.com/rawatsandeep670/whiteboard-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "keywords": [],
  "scripts": {
    "build": "microbundle-crl --compress --no-sourcemap --define process.env.NODE_ENV=production --format modern,cjs",
    "build:dev": "microbundle-crl --compress --no-sourcemap --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@commitlint/travis-cli": "^11.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/free-regular-svg-icons": "^5.13.0",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.20.4",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "html2canvas": "^1.0.0-rc.5",
    "husky": "^4.3.0",
    "jquery": "^3.2.1",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "standard-version": "^9.0.0",
    "vanilla-picker": "^2.10.1"
  },
  "commitlint": {
    "extends": [
      "./@commitlint/config-conventional",
      "./@commitlint/config-lerna-scopes"
    ]
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
